Machine.WorkArea.size(xmin=-10,xmax=10,ymin=-10,ymax=10);

draw {
    S;
}

function point AA(number NN, number MM){
    return (NN,MM);
}

shape S{
    point Offset = (0,0);
    number sideVal = 0;
    number b = 0
    repeat(10)
        sideVal = sideVal+1;
        Offset = AA(sideVal,b); 
        line.from(Offset).to(10,10);
    repeat.end;
    
}