Machine.WorkArea.size(xmin=-10,xmax=10,ymin=-10,ymax=10);

draw {
    S;
}

function number AA(number NN){
return 2 * NN;
}

shape S{
    number xOffset = 0;
    number sideVal = 0;
    number b = 0
    repeat(10)
        b = AA(sideVal+2);
        sideVal = sideVal+1;
        xOffset = AA(sideVal); 
        
    repeat.end;
    
}